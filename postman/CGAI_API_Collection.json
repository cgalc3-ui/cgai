									"    var jsonData = pm.response.json();",
									"    if (jsonData.success && jsonData.token) {",
									"        pm.environment.set('token', jsonData.token);",
									"        pm.environment.set('user_id', jsonData.user.id);",
									"        pm.environment.unset('temp_token');",
									"    }",
									"}"
								],
								"type": "text/javascript"
							}
						}
					],
